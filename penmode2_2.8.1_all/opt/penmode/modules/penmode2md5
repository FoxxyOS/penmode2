prog="PH#0S Penmode"
result=""
Encoding=UTF-8
cd /opt/penmode/modules/modulomd5crack/
[ -z $GTKDIALOG ] && GTKDIALOG=gtkdialog

MAIN_DIALOG='
<window title="Penmode 2-MD5" resizable="false" image-name="/opt/penmode/icons/icon.png" >
        <vbox>
		<pixmap>
					<input file>/opt/penmode/icons/redenti2.png</input>
				</pixmap>
		<frame CRACK WEB>		
		<hbox>		
		<text>
				<label>MD5:</label>
			</text>
        <entry>
			<variable>MD5</variable>
		</entry>
		<button>
		<input file>/opt/penmode/icons/lente.png</input>
		<width>30</width>
        <label> SEARCH </label>
          <action>xterm -geometry 80x50 -hold -e /opt/penmode/modules/modulomd5crack/crackweb &</action>
        </button>
	 </hbox>
	 <hbox>
	 <terminal has-focus="true"
			    argv0="/bin/bash">
				<height>20</height>
                <width>50</width>

				<input>echo ./md5.sh </input>

			</terminal>
	 </hbox>
	 </frame>			
	 </vbox>
</window>
'
export MAIN_DIALOG

case $1 in
	-d | --dump) echo "$MAIN_DIALOG" ;;
	*) $GTKDIALOG --program=MAIN_DIALOG ;;
esac
